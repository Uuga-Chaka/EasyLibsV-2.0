<resources>

    <!-- Base application theme. -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/indentityColorOne</item>
        <item name="colorPrimaryDark">@color/indentityColorOne</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:fontFamily">@font/nunito</item>
    </style>

    <style name="AppTheme" parent="AppBaseTheme">
    <item name="android:editTextStyle">@style/et_style</item>

    </style>

    <style name="et_style" parent="android:Widget.Holo.EditText">
        <item name="android:background">@drawable/et_style</item>
        <item name="android:textColor">#808080</item>
        <item name="android:layout_height">45dip</item>
    </style>
    
</resources>
